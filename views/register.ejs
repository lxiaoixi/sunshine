    function cityChange() {
      city = $("#city").find("option:selected").text();
      console.log(city)
      var select = document.querySelector("#company");
      if(city == "请选择您所在的城市"){
        select.setAttribute("style",'display:none');
      }else{
        select.setAttribute("style",'display:block');
        citys[city].forEach(function(item,index){
          select.options[parseInt(index)+1] = new Option(item,item);
        })
      }
    }

    function companyChange(){
      const company = $("#company").find("option:selected").text();
      const firstLevel = ["山西省人民检察院","山西省人民检察院太原铁路运输分院"];
